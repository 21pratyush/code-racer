andList = React.forwardRef<\n  React.ElementRef<typeof CommandPrimitive.List>,\n  React.ComponentPropsWithoutRef<typeof CommandPrimitive.List>\n>(({ className, ...props }, ref) => (\n  <CommandPrimitive.List\n    ref={ref}\n    className={cn(\"max-h-[300px] overflow-y-auto overflow-x-hidden\", className)}\n    {...props}\n  />\n))\n\nCommandList.displayName = CommandPrimitive.List.displayName\n\nconst CommandEmpty = React.forwardRef<\n  React.ElementRef<typeof CommandPrimitive.Empty>,\n  React.ComponentPropsWithoutRef<typeof CommandPrimitive.Empty>\n>((props, ref) => (\n  <CommandPrimitive.Empty\n    ref={ref}\n    className=\"py-6 text-center text-sm\"\n    {...props}\n  />\n))\n\nCommandEmpty.displayName = CommandPrimitive.Empty.displayName\n\nconst CommandGroup = React.forwardRef<\n  React.ElementRef<typeof CommandPrimitive.Group>,\n  React.ComponentPropsWithoutRef<typeof CommandPrimitive.Group>\n>(({ className, ...props }, ref) => (\n  <CommandPrimitive.Group\n    ref={ref}\n    className={cn(\n      \"overflow-hidden p-1 text-foreground [&_[cmdk-group-heading]]:px-2 [&_[cmdk-group-heading]]:py-1.5 [&_[cmdk-group-heading]]:text-xs [&_[cmdk-group-heading]]:font-medium [&_[cmdk-group-heading]]:text-muted-foreground\",\n      className\n    )}\n    {...props}\n  />\n))\n\nCommandGroup.displayName = CommandPrimitive.Group.displayName\n\nconst CommandSeparator = React.forwardRef<\n  React.ElementRef<typeof CommandPrimitive.Separator>,\n  React.ComponentPropsWithoutRef<typeof CommandPrimitive.Separator>\n>(({ className, ...props }, ref) => (\n  <CommandPrimitive.Separator\n    ref={ref}\n    className={cn(\"-mx-1 h-px bg-border\", className)}\n    {...props}\n  />\n))\nCommandSeparator.displayName = CommandPrimitive.Separator.displayName\n\nconst CommandItem = React.forwardRef<\n  React.ElementRef<typeof CommandPrimitive.Item>,\n  React.ComponentPropsWithoutRef<typeof CommandPrimitive.Item>\n>(({ className, ...props }, ref) => (\n  <CommandPrimitive.Item\n    ref={ref}\n    className={cn(\n      \"relative flex cursor-default select-none items-center rounded-sm px-2 py-1.5 text-sm outline-none aria-selected:bg-accent aria-selected:text-accent-foreground data-[disabled]:pointer-events-none data-[disabled]:opacity-50\",\n      className\n    )}\n    {...props}\n  />\n))\n\nCommandItem.displayName = CommandPrimitive.Item.displayName\n\nconst CommandShortcut = ({\n  className,\n  ...props\n}: React.HTMLAttributes<HTMLSpanElement>) => {\n  return (\n    <span\n      className={cn(\n        \"ml-auto text-xs tracking-widest text-muted-foreground\",\n        className\n      )}\n      {...props}\n    />\n  )\n}\nCommandShortcut.displayName = \"CommandShortcut\"\n\nexport {\n  Command,\n  CommandDialog,\n  CommandInput,\n  CommandList,\n  CommandEmpty,\n  CommandGroup,\n  CommandItem,\n  CommandShortcut,\n  CommandSeparator,\n}\n","/home/sycodes95/personal-projects/code-racer/src/components/ui/dialog.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/dropdown-menu.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/form.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/heading.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/hover-card.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/input.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/label.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/popover.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/progress-bar.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/scroll-area.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/select.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/sheet.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/skeleton.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/spinner.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/sub-heading.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/table.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/tabs.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/textarea.tsx",["810"],[],"import * as React from \"react\"\n\nimport { cn } from \"@/lib/utils\"\n\nexport interface TextareaProps\n  extends React.TextareaHTMLAttributes<HTMLTextAreaElement> {}\n\nconst Textarea = React.forwardRef<HTMLTextAreaElement, TextareaProps>(\n  ({ className, ...props }, ref) => {\n    return (\n      <textarea\n        className={cn(\n          \"flex min-h-[80px] w-full rounded-md border border-input bg-transparent px-3 py-2 text-sm ring-offset-background placeholder:text-muted-foreground focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-ring focus-visible:ring-offset-2 disabled:cursor-not-allowed disabled:opacity-50\",\n          className\n        )}\n        ref={ref}\n        {...props}\n      />\n    )\n  }\n)\nTextarea.displayName = \"Textarea\"\n\nexport { Textarea }\n","/home/sycodes95/personal-projects/code-racer/src/components/ui/time.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/toast.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/toaster.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/tooltip.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/components/ui/use-toast.ts",["811"],[],"// Inspired by react-hot-toast library\n\"use client\"\n\nimport type {\n  ToastActionElement,\n  ToastProps,\n} from \"@/components/ui/toast\"\nimport { useEffect, useState } from \"react\"\n\nconst TOAST_LIMIT = 2\nconst TOAST_REMOVE_DELAY = 1000000\n\ntype ToasterToast = ToastProps & {\n  id: string\n  title?: React.ReactNode\n  description?: React.ReactNode\n  action?: ToastActionElement\n}\n\nconst actionTypes = {\n  ADD_TOAST: \"ADD_TOAST\",\n  UPDATE_TOAST: \"UPDATE_TOAST\",\n  DISMISS_TOAST: \"DISMISS_TOAST\",\n  REMOVE_TOAST: \"REMOVE_TOAST\",\n} as const\n\nlet count = 0\n\nfunction genId() {\n  count = (count + 1) % Number.MAX_VALUE\n  return count.toString()\n}\n\ntype ActionType = typeof actionTypes\n\ntype Action =\n  | {\n      type: ActionType[\"ADD_TOAST\"]\n      toast: ToasterToast\n    }\n  | {\n      type: ActionType[\"UPDATE_TOAST\"]\n      toast: Partial<ToasterToast>\n    }\n  | {\n      type: ActionType[\"DISMISS_TOAST\"]\n      toastId?: ToasterToast[\"id\"]\n    }\n  | {\n      type: ActionType[\"REMOVE_TOAST\"]\n      toastId?: ToasterToast[\"id\"]\n    }\n\ninterface State {\n  toasts: ToasterToast[]\n}\n\nconst toastTimeouts = new Map<string, ReturnType<typeof setTimeout>>()\n\nconst addToRemoveQueue = (toastId: string) => {\n  if (toastTimeouts.has(toastId)) {\n    return\n  }\n\n  const timeout = setTimeout(() => {\n    toastTimeouts.delete(toastId)\n    dispatch({\n      type: \"REMOVE_TOAST\",\n      toastId: toastId,\n    })\n  }, TOAST_REMOVE_DELAY)\n\n  toastTimeouts.set(toastId, timeout)\n}\n\nexport const reducer = (state: State, action: Action): State => {\n  switch (action.type) {\n    case \"ADD_TOAST\":\n      return {\n        ...state,\n        toasts: [action.toast, ...state.toasts].slice(0, TOAST_LIMIT),\n      }\n\n    case \"UPDATE_TOAST\":\n      return {\n        ...state,\n        toasts: state.toasts.map((t) =>\n          t.id === action.toast.id ? { ...t, ...action.toast } : t\n        ),\n      }\n\n    case \"DISMISS_TOAST\": {\n      const { toastId } = action\n\n      // ! Side effects ! - This could be extracted into a dismissToast() action,\n      // but I'll keep it here for simplicity\n      if (toastId) {\n        addToRemoveQueue(toastId)\n      } else {\n        state.toasts.forEach((toast) => {\n          addToRemoveQueue(toast.id)\n        })\n      }\n\n      return {\n        ...state,\n        toasts: state.toasts.map((t) =>\n          t.id === toastId || toastId === undefined\n            ? {\n                ...t,\n                open: false,\n              }\n            : t\n        ),\n      }\n    }\n    case \"REMOVE_TOAST\":\n      if (action.toastId === undefined) {\n        return {\n          ...state,\n          toasts: [],\n        }\n      }\n      return {\n        ...state,\n        toasts: state.toasts.filter((t) => t.id !== action.toastId),\n      }\n  }\n}\n\nconst listeners: Array<(state: State) => void> = []\n\nlet memoryState: State = { toasts: [] }\n\nfunction dispatch(action: Action) {\n  memoryState = reducer(memoryState, action)\n  listeners.forEach((listener) => {\n    listener(memoryState)\n  })\n}\n\ntype Toast = Omit<ToasterToast, \"id\">\n\nfunction toast({...props}: Toast, toastId?: string) {\n  if (!toastId) {\n    toastId = genId();\n  }\n  const update = (props: ToasterToast) =>\n    dispatch({\n      type: \"UPDATE_TOAST\",\n      toast: { ...props, id: toastId },\n    })\n  const dismiss = () => dispatch({ type: \"DISMISS_TOAST\", toastId: toastId })\n\n  dispatch({\n    type: \"ADD_TOAST\",\n    toast: {\n      ...props,\n      id: toastId,\n      open: true,\n      onOpenChange: (open) => {\n        if (!open) dismiss()\n      },\n    },\n  })\n\n  return {\n    id: toastId,\n    dismiss,\n    update,\n  }\n}\n\nfunction useToast() {\n  const [state, setState] = useState<State>(memoryState)\n\n  useEffect(() => {\n    listeners.push(setState)\n    return () => {\n      const index = listeners.indexOf(setState)\n      if (index > -1) {\n        listeners.splice(index, 1)\n      }\n    }\n  }, [state])\n\n  return {\n    ...state,\n    toast,\n    dismiss: (toastId?: string) => dispatch({ type: \"DISMISS_TOAST\", toastId }),\n  }\n}\n\nexport { useToast, toast }\n","/home/sycodes95/personal-projects/code-racer/src/components/user-dropdown.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/config/languages.ts",[],[],"/home/sycodes95/personal-projects/code-racer/src/config/site.ts",[],[],"/home/sycodes95/personal-projects/code-racer/src/context/confetti.tsx",[],[],"/home/sycodes95/personal-projects/code-racer/src/lib/actions.ts",["812"],[],"import { type z, ZodError } from \"zod\";\n\ntype ActionType<InputType extends z.ZodTypeAny, ResponseType> = (\n  input: z.infer<InputType>,\n) => Promise<ResponseType>;\n\ntype validatedAction<InputType extends z.ZodTypeAny, ResponseType> = ActionType<\n  InputType,\n  ResponseType\n>;\n\nexport function safeAction<InputType extends z.ZodTypeAny>(\n  validator?: InputType,\n) {\n  return function <ResponseType>(\n    action: ActionType<InputType, ResponseType>,\n  ) {\n    const validatedAction = async (input: z.infer<InputType>) => {\n      if (validator) {\n        try {\n          validator.parse(input);\n        } catch (err) {\n          if (err instanceof ZodError) {\n            throw new Error(err.issues.map((i) => i.message).join(\"\\n\"));\n          } else {\n            throw new Error(\"Something went wrong...\");\n          }\n        }\n      }\n      return await action(input);\n    };\n\n    return validatedAction as validatedAction<InputType, ResponseType>;\n  };\n}\n","/home/sycodes95/personal-projects/code-racer/src/lib/exceptions/custom-hooks.ts",[],[],"/home/sycodes95/personal-projects/code-racer/src/lib/prisma.ts",[],[],"/home/sycodes95/personal-projects/code-racer/src/lib/session.ts",[],[],"/home/sycodes95/personal-projects/code-racer/src/lib/utils.ts",[],[],"/home/sycodes95/personal-projects/code-racer/src/lib/validations/snippet.ts",[],[],"/home/sycodes95/personal-projects/code-racer/src/middleware.ts",[],[],"/home/sycodes95/personal-projects/code-racer/src/types/nav.ts",[],[],"/home/sycodes95/personal-projects/code-racer/src/types/next-auth.d.ts",[],[],"/home/sycodes95/personal-projects/code-racer/src/types/result.ts",[],[],"/home/sycodes95/personal-projects/code-racer/src/app/contributors/page.ts