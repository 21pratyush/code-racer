generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @map("created_at") @default(now())
  displayName String?  @map("display_name")
  photoURL    String?  @map("photo_url")

  @@map("users")
}
